public class Solution {
	public double findMedianSortedArrays(int A[], int B[]) {
		int count = A.length + B.length;
		int target = count / 2;
		int i = 0, j = 0;
		int current = 0;
		int result = 0;
		while (current < target && i < A.length && j < B.length) {
			if (A[i] < B[j] && i < A.length) {
				result = A[i];
				i++;
			} else {
				result = B[j];
				j++;
			}
			current++;
		}
		return result;
	}
}