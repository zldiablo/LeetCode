public class Solution {
	public double findMedianSortedArrays(int A[], int B[]) {
		int count = A.length + B.length;
		int target = count / 2;
		int i = 0, j = 0;
		int current = 0;
		while (i < A.length && j < B.length) {
			if (A[i]<B[j]){
				i++;
			}else {
				j++;
			}
		}
		return 0;
	}
}